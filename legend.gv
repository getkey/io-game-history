digraph legend {
	labelloc=t;
    label=<<b>Legend</b>>;
	rankdir=LR;

	a [style=invis]
	b [style=invis];

	".io game" [color="#90caf9", style="rounded, filled", shape=box];
	"not .io game\n(by modern standards)" [color="#E1FA91", style="rounded, filled", shape=box];

	a -> b [label="gameplay \"inspiration\""];
}
